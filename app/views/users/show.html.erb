<%= @user.email %>
<hr>
<h3> spaces you are the sole manager of </h3>
<ul>
  <% @user.managed_spaces.each do |space| %>
    <li> <%= space.name %> | <%= link_to "manage this space", "#" %>
  <% end %>
</ul>
<hr>
<h3> spaces you are a member of </h3>
<ul>
  <% @user.member_spaces.each do |space| %>
    <li> <%= space.name %> | <%= link_to "view this space", organization_path(space.id) %>
  <% end %>
</ul>
<hr>