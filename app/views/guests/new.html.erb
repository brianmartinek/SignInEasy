<%= @guest %>
<%= @organization_id %>

<%= form_for @guest do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= label_tag :employee_name %>
  <%= text_field_tag 'guest[employee_name]' %>

  <%= f.hidden_field :organization_id, value: @organization_id %>

  <%= f.submit %>

<% end %>