%h1 SignInEasy Master Admin Page
%hr
%ul.admin-table
  -@objects.each do |object|
    %li    
      %h3= object.model
      %table.table.table-striped
        %thead
          -important_columns(object).each do |column|
            %th 
              = column
          %th Edit
          %th Destroy
        %tbody
          -object.each do |obj|
            %tr
              -important_attributes(obj).each do |key, value|
                %td
                  = value
              %td 
                <a href="http://localhost:3000/#{asset_url(obj)}" method="put"><i class="fa fa-edit"></i></a>
              %td
                <a href="http://localhost:3000/#{asset_url(obj)}" method="delete"><i class="fa fa-times"></i></a>